{
  "version": 2,
  "name": "giggsi-digital-menu",
  "alias": ["giggsi-menu"],
  "scope": "nioasofts-projects",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@giggsi_supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@giggsi_supabase_anon_key"
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}